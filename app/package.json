{
  "name": "social-parser",
  "productName": "social parser",
  "version": "3.0.0",
  "main": "public/electron.js",
  "description": "Shows the metric of site visits and metrics from VK, Telegram, YouTube, Ok, Instagram and Twitter",
  "homepage": "./",
  "private": true,
  "author": "Anton Gorban <antogor.work@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonGorban/social-parser.git"
  },
  "bugs": {
    "url": "https://github.com/AntonGorban/social-parser/issues"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"set BROWSER=none && npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.56",
    "@electron-forge/maker-deb": "^6.0.0-beta.56",
    "@electron-forge/maker-rpm": "^6.0.0-beta.56",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.56",
    "@electron-forge/maker-zip": "^6.0.0-beta.56",
    "concurrently": "^6.1.0",
    "electron": "^12.0.9",
    "electron-devtools-installer": "^3.2.0",
    "wait-on": "^5.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/icons/win/icon.ico",
        "name": "Social parser",
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "./src/icons/win/icon.ico"
      },
      "electronWinstallerConfig": {
        "name": "social_parser",
        "icon": "./src/icons/win/icon.ico"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "AntonGorban",
        "name": "social-parser"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "socialParser"
      }
    }
  }
}
